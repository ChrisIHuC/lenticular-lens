{
  "ECARTICO_FULL_NAME": {
    "sql_template": "ecartico_full_name({property})"
  },
  "PARSE_DATE": {
    "sql_template": "to_date_immutable({property}, {format})"
  },
  "PARSE_NUMERIC": {
    "sql_template": "to_numeric_immutable({property})"
  },
  "PREFIX": {
    "sql_template": "{prefix} || {property}"
  },
  "SUFFIX": {
    "sql_template": "{property} || {suffix}"
  },
  "REPLACE": {
    "sql_template": "replace({property}, {from}, {to})"
  }
}